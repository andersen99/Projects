<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>com.servlets.LoginController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>guest</servlet-name>
    <servlet-class>com.servlets.GuestController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>guest</servlet-name>
    <url-pattern>/guest</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>host</servlet-name>
    <servlet-class>com.servlets.HostController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>host</servlet-name>
    <url-pattern>/host</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>reserveroom</servlet-name>
  	<servlet-class>com.servlets.ReserveRoomServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>reserveroom</servlet-name>
  	<url-pattern>/reserveroom</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>raiseissue</servlet-name>
    <servlet-class>com.servlets.RaiseIssueServlet</servlet-class>
  </servlet>
  <servlet>
  	<display-name>ReviewReservationServlet</display-name>
  	<servlet-name>ReviewReservationServlet</servlet-name>
  	<servlet-class>com.servlets.ReviewReservationServlet</servlet-class>
  </servlet>
  <servlet>
  	<display-name>ReviewIssuesServlet</display-name>
  	<servlet-name>ReviewIssuesServlet</servlet-name>
  	<servlet-class>com.servlets.ReviewIssuesServlet</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>CreateAccountServlet</display-name>
  	<servlet-name>CreateAccountServlet</servlet-name>
  	<servlet-class>com.servlets.CreateAccountServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>raiseissue</servlet-name>
    <url-pattern>/raiseissue</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReviewReservationServlet</servlet-name>
  	<url-pattern>/reviewreservation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReviewIssuesServlet</servlet-name>
  	<url-pattern>/reviewissues</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreateAccountServlet</servlet-name>
  	<url-pattern>/createaccount</url-pattern>
  </servlet-mapping>
</web-app>
